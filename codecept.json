{
  "output": "./test/output",
  "helpers": {
    "Puppeteer": {
      "url": "http://52.214.26.93/en",
      "show": false,
      "restart": false,
      "waitForAction": 1000,
      "keepBrowserState": true,
      "uniqueScreenshotNames": "false",
      "windowSize": "1200x980",
      "chrome": {
        "args": [
          "--no-sandbox"
        ]
      }
    },
    "Mochawesome": {
      "uniqueScreenshotNames": "false"
    }
  },
  "include": {
    "I": "./test/stepDefinitions/steps_file.js",
    "basePage": "./test/pages/Base.js",
    "homePage": "./test/pages/Home.js",
    "networkPage": "./test/pages/Network.js"
  },
  "mocha": {
    "reporterOptions": {
      "codeceptjs-cli-reporter": {
        "stdout": "-",
        "options": {
          "verbose": false,
          "steps": true
        }
      },
      "mochawesome": {
        "stdout": "./test/output/console.log",
        "options": {
          "reportDir": "./test/output",
          "reportFilename": "report"
        }
      }
    }
  },
  "multiple": {
    "parallel": {
      "chunks": 2,
      "browsers": ["chrome"]
    }
  },
  "bootstrap": false,
  "teardown": null,
  "hooks": [],
  "tests": "./test/features/**/*_test.js",
  "timeout": 10000,
  "name": "codecept"
}
